      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand font" href="/">WL Makerspace</a>
    </div>
    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
      <ul class="nav navbar-nav">
      <%if session[:visited]%>
          <li>
            <a class="font" href="/#about"> About us</a>
          </li>
          <li>
            <a class="font" href="/blog"> Blog</a>
          </li>
          <li>
            <a class="font" href="/dashboard">Dashboard</a>
          </li>


      <%else%>
          <li>
            <a class="font" href="/#about"> About us</a>
          </li>
          <li>
            <a class="font" href="/blog"> Blog</a>
          </li>
          <li>
            <a class="font" href="/#contact">Request Training</a>
          </li>
      <%end  %>
      </ul>
      <ul class="nav navbar-nav navbar-right">

        <%if session[:visited] %>
            <li class="dropdown">
              <a href="#" class="dropdown-toggle font" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><%=@user.firstname %> <%= @user.lastname %><span class="caret"></span></a>
              <ul class="dropdown-menu">
                <li><a class="font" href="/settings"><i class="fa fa-cog"></i> Settings</a></li>

                <li role="separator" class="divider"></li>
                <li><a class="font" href="/logout"><i class="fa fa-sign-out"></i> Logout</a></li>
              </ul>

        <%else%>
            <a  data-toggle="modal" data-target="#myModal">
              <li>
                Coach Login
            </a>
        <%end  %>
        </li>
      </ul>
    </div>
    <!-- /.navbar-collapse -->
  </div>
  <!-- /.container -->
</nav>










<%=yield%>
<!--Modals-->
<%if session[:visited] %>
<div class="modal fade perms" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <form method="post" action="/perms/edit">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
          <h4 class="modal-title" id="myModalLabel">Edit Permissions</h4>
        </div>
        <div class="modal-body">
          <div class="form-group">
            <label>Webmaster Password</label>
            <input class="form-control" type="text" name="permpassword" placeholder="Password">
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
          <button type="submit" class="btn btn-primary">Login</button>
        </div>
      </form>
    </div>
  </div>
</div>


    <%end %>
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <form method="post" action="/user/auth">
            <fieldset>
              <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">Coach Login</h4>
              </div>
              <div class="modal-body">
                <div class="form-group">
                  <label>E-Mail</label>
                  <input class="form-control" required type="email" name="email" placeholder="EX: johnsmith@domain.com">
                </div>
                <div class="form-group">
                  <label>Password</label>
                  <input class="form-control" required type="password" name="password" placeholder="Password">
                </div>
              </div>
              <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                <button type="submit" class="btn btn-primary">Login</button>
              </div>
            </fieldset>
          </form>
        </div>
      </div>
    </div>
